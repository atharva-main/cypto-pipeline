            "datasource": {
              "type": "postgres",
              "uid": "${DS_POSTGRESQL}"
            },
            "format": "table",
            "group": [],
            "metricColumn": "none",
            "rawQuery": true,
            "rawSql": "SELECT symbol, price, quantity, trade_time FROM raw_trades WHERE trade_time >= NOW() - INTERVAL '10 minutes' ORDER BY trade_time DESC LIMIT 50",
            "refId": "A",
            "select": [
              [
                {
                  "params": ["price"],
                  "type": "column"
