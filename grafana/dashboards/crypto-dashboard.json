      {
        "id": 4,
        "title": "Trade Count per Minute",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${DS_POSTGRESQL}"
            },
            "format": "table",
            "group": [],
            "metricColumn": "none",
            "rawQuery": true,
            "rawSql": "SELECT symbol, trade_count as \"Trades/Min\" FROM processed_trades WHERE window_start >= NOW() - INTERVAL '5 minutes' ORDER BY window_start DESC, trade_count DESC LIMIT 10",
            "refId": "A",
            "select": [
              [
                {
                  "params": ["trade_count"],
                  "type": "column"
                }
              ]
            ],
            "table": "processed_trades",
            "timeColumn": "window_start",
            "timeColumnType": "timestamp",
            "where": [
              {
                "name": "$__timeFilter",
                "params": [],
                "type": "macro"
              }
            ]
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            },
            "unit": "short"
