{
  "dashboard": {
    "id": null,
    "title": "Crypto Trading Pipeline Dashboard",
    "tags": ["crypto", "trading", "real-time"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Live Price Updates",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${DS_POSTGRESQL}"
            },
            "format": "table",
            "group": [],
            "metricColumn": "none",
            "rawQuery": true,
            "rawSql": "SELECT symbol, current_price as \"Price\", price_change_24h as \"24h Change %\" FROM latest_market_data ORDER BY symbol",
            "refId": "A",
            "select": [
              [
                {
                  "params": ["current_price"],
                  "type": "column"
                }
              ]
            ],
            "table": "market_summary",
            "timeColumn": "last_updated",
            "timeColumnType": "timestamp",
            "where": [
              {
                "name": "$__timeFilter",
                "params": [],
                "type": "macro"
              }
            ]
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 0
                }
              ]
            },
            "unit": "currencyUSD"
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
